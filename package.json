    "@types/jasminewd2": "~2.0.3",
    "@types/knex": "^0.14.14",
    "@types/leaflet": "^1.2.7",
    "@types/leaflet.awesome-markers": "^2.0.23",
    "@types/leaflet.locatecontrol": "^0.60.6",
    "@types/node": "~8.9.4",
    "@types/pino": "^4.16.0",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
