{
  "name": "blue",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "watch:client": "ng serve --host 0.0.0.0 --disable-host-check",
    "watch:server": "nodemon -e ts -w 'server/**/*.ts' --ignore 'server/**/*.spec.ts' -x npm run start:server ",
    "lastVersion": "git pull && yarn",
    "start": "npm run build && npm run start:server",
    "start:server": "node -r ts-node/register server/server.ts",
    "migrate": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --cwd server --knexfile knexfile.ts",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.2.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/pwa": "^0.7.0-beta.1",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.0.3",
    "@asymmetrik/ngx-leaflet": "^3.0.2",
    "apollo-angular": "^1.1.0",
    "apollo-angular-link-http": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "core-js": "^2.5.4",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "fastify": "^1.5.0",
    "fastify-static": "^0.12.0",
    "graphql-tag": "^2.9.2",
    "knex": "^0.14.6",
    "leaflet": "^1.3.1",
    "leaflet.awesome-markers": "^2.0.4",
    "leaflet.locatecontrol": "^0.62.0",
    "leaflet.offline": "^1.0.3",
    "pg": "^7.4.3",
    "pino": "^4.17.3",
    "postgraphile": "^4.0.0-beta.10",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
